{
  "server_config_filename" : "crabjs.json",
  "server_port": 3000,
  "server_timeout": 300000,
  "server_controllers_path": "controller",
  "server_entities_path": "entity",
  "debug" : {
    "level" : "error"
  },
  "security": {
    "jwt": {
      "tokenSignInRoute": "/auth/signin",
      "tokenField": "access_token",
      "signClientIdField" : "client_id",
      "signClientSecretField" : "client_secret",
      "tokenBearer": true,
      "tokenExpires" : 300,
      "tokenReplaceNew" : true,
      "resetUserTokensWhenLogin": true,
      "refreshToken" : {
        "tokenExpires" : 18000,
        "tokenSecret" : null,
        "tokenReplaceNew" : true
      }
    }
  },
  "language": "en",
  "localStorage" : {
      "type" : "file",
      "filename" : "",
      "path" : "localstorage"
  },
  "cacheStoragePath" : "cache"
}